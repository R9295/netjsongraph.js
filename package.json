{
  "name": "netjsongraph.js",
  "version": "0.1.3",
  "description": "NetJson format data visualization",
  "main": "./dist/netjsongraph.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "test": "jest",
    "dev": "concurrently \"serve\" \"webpack -w\"",
    "build": "rm -rf ./dist && webpack",
    "prepare": "npm run build",
    "cover": "npm run test -- --coverage && cat ./coverage/lcov.info | coveralls",
    "precommit": "lint-staged",
    "lint": "eslint --fix --ext .js src"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "https://github.com/netjson/netjsongraph.js.git",
  "author": "Federico Capoano <federico.capoano@gmail.com> (http://nemesisdesign.net/)",
  "contributors": [
    "GeekPlux <geekplux@gmail.com> (http://geekplux.com/)"
  ],
  "license": "BSD-3-Clause",
  "keywords": [
    "netjson",
    "network",
    "data",
    "visualization"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.4.4",
    "@types/d3": "^6.3.0",
    "@types/leaflet": "^1.7.0",
    "@types/node": "^14.14.37",
    "acorn": "^6.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^6.0.2",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "lint-staged": "^7.2.2",
    "prettier": "1.14.2",
    "serve": "^11.3.2",
    "ts-loader": "^8.1.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "d3": "^6.6.2",
    "deepmerge": "^4.2.2",
    "leaflet": "^1.7.1"
  }
}
